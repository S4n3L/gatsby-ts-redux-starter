{
  "name": "gatsby-ts-redux-starter",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.1.0",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.7",
    "autoprefixer": "^10.1.0",
    "css-loader": "^5.0.1",
    "gatsby": "^2.19.7",
    "gatsby-cli": "^2.10.2",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sharp": "^2.4.3",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.13",
    "immer": "^8.0.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^10.1.0",
    "sass-resources-loader": "^2.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^2.2.1",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^@static/(.*)$": "<rootDir>/src/static/$1",
      "^@styles/(.*)$": "<rootDir>/src/styles/$1",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/mocks/fileMock.ts",
      "\\.(svg)$": "<rootDir>/src/tests/mocks/svgrMock.tsx",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --verbose --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
